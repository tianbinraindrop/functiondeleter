{
  "name": "pythonfunctiondeleter",
  "displayName": "python function/class deleter",
  "description": "remove/duplicate python function/class",
  "version": "0.0.7",
  "engines": {
    "vscode": "^1.87.0"
  },
  "publisher": "Tianbin",
  "repository": {
    "url": "https://github.com/tianbinraindrop/functiondeleter"
  },
  "keywords": [
    "refactor",
    "javascript",
    "typescript",
    "file  content",
    "file  exchange",
    "multi-file operation",
    "line exchange",
    "function deletion",
    "class deletion"
  ],
  "categories": [
    "Programming Languages",
    "Formatters"
  ],
  "main": "./out/extension.js",
  "activationEvents": [
    "onLanguage:python",
    "onLanguage:mardown"
  ],
  "contributes": {
    "commands": [
      {
        "command": "extension.pythonfunctiondeleter",
        "title": "remove python function/class"
      },      
      {
        "command": "extension.pythonfunctionduplicate",
        "title": "duplicate python function/class"
      },
      {
        "command": "extension.movecurrentlinetorighteditor",
        "title": "move current line to right editor",
        "icon":{
          "dark": "images/dark/movecurrentlinetorighteditor.svg",
          "light": "images/light/movecurrentlinetorighteditor.svg"
        }
      }
    ],
    "keybindings": [
      {
        "command": "extension.pythonfunctiondeleter",
        "key": "ctrl+f5"
      },
      {
        "command": "extension.pythonfunctionduplicate",
        "key": "ctrl+f6"
      },
      {
        "command": "extension.movecurrentlinetorighteditor",
        "key": "ctrl+f7"
      }
    ]
  },
  "icon": "images/icon.png",
  "scripts": {
    "vscode:prepublish": "npm run compile",
    "compile": "tsc -p ./",
    "watch": "tsc -watch -p ./",
    "pretest": "npm run compile && npm run lint",
    "lint": "eslint src --ext ts",
    "test": "vscode-test"
  },
  "dependencies": {
    "@types/mocha": "^10.0.6",
    "@types/node": "^18.19.24",
    "@types/vscode": "^1.87.0",
    "@typescript-eslint/eslint-plugin": "^7.0.2",
    "@typescript-eslint/parser": "^7.0.2",
    "@vscode/test-cli": "^0.0.6",
    "@vscode/test-electron": "^2.3.9",
    "dt-python-parser": "^0.9.0",
    "eslint": "^8.56.0",
    "typescript": "^5.3.3"
  }
}
